{
  "name": "react-oui-icons",
  "version": "0.8.0",
  "main": "lib/react-oui-icons.js",
  "private": false,
  "dependencies": {
    "fs-extra": "3.0.1",
    "object-assign": "4.1.1",
    "promise": "7.1.1"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.2.12",
    "@storybook/addon-knobs": "^3.2.12",
